package escola;

public class Aluno {
	//SEM O METODO GETTER E SETTER
	//String nome;
	//int idade;
	
	
	//COM O METODO GETTER E SETTER
	
	private String nome;
	private int idade;
	
	public String getNome() {
		return nome;
	}
	
	public void setNome(String newNome) {
		nome = newNome;
	}
	
	public int getIdade() {
		return idade;
	}
	
	public void setIdade(int newIdade) {
		idade = newIdade;
	}
}

