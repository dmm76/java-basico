package iphone;

import iphone.musica.ReprodutorMusical;
import iphone.navegador.NavegadorInternet;

public class IphoneDouglas {
	public static void main(String[] args) {
		ReprodutorMusical player = new ReprodutorMusical();
		player.tocarMusica();
		player.pausarMusica();
		
		AparelhoTelefonico phone = new AparelhoTelefonico();
		phone.ligar();
		phone.atender();
		phone.correio();
		
		NavegadorInternet browser = new NavegadorInternet();
		browser.exibindoPagina();
		browser.adicionarAba();
		browser.avancarPagina();
		browser.atualizarPagina();
		browser.voltandoPagina();
		
		GoogleChrome chrome = new GoogleChrome();
		chrome.exibindoPagina();
		
	}
	
}
